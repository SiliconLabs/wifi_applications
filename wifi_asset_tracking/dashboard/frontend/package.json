{
  "name": "silabs",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx  --quiet && echo 'Linting passed.' || echo 'Linting failed.'",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx  --quiet --fix && echo 'Linting and fixing passed.' || (echo 'Linting and fixing failed.'; exit 1)",
    "precommit": "lint-staged",
    "preview": "vite preview",
    "test": "vitest --coverage"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "__tests__/*.{js,jsx,ts,tsx}": [
      "vitest --verbose --watch"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "dependencies": {
    "@azure/msal-browser": "^3.14.0",
    "@azure/msal-react": "^2.0.16",
    "@reduxjs/toolkit": "^2.2.5",
    "@tabler/icons-react": "^3.5.0",
    "axios": "^1.7.4",
    "bootstrap": "^5.3.3",
    "moment": "^2.30.1",
    "react": "^18.2.0",
    "react-apexcharts": "^1.4.1",
    "react-bootstrap": "^2.10.2",
    "react-bootstrap-icons": "^1.11.4",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.23.1",
    "react-toastify": "^10.0.5",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^3.1.0",
    "socket.io-client": "^4.7.5",
    "vite-plugin-svgr": "^4.2.0",
    "websocket": "^1.0.35"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@types/redux-logger": "^3.0.13",
    "@types/redux-mock-store": "^1.0.6",
    "@types/websocket": "^1.0.10",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.5.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jsdom": "^24.1.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "sass": "^1.72.0",
    "typescript": "^5.2.2",
    "vite": "^5.1.6",
    "vitest": "^1.4.0"
  }
}
