  from: third_party_hw_drivers


define:
  - name: DEBUG_EFM
  - name: SPI_MULTI_SLAVE
  - name: SLI_SI91X_MCU_MOV_ROM_API_TO_FLASH

tag:
  - hardware:rf:band:2400

sdk_extension:
- id: third_party_hw_drivers
  version: 3.0.0
- id: wiseconnect3_sdk
  version: 3.2.0

ui_hints:
