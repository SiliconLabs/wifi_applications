  from: wiseconnect3_sdk
- id: sl_ulp_timer
  from: wiseconnect3_sdk
- id: sntp
  from: wiseconnect3_sdk
- id: syscalls
  from: wiseconnect3_sdk
- id: ulp_timers_instance
  instance: [timer0]
  from: wiseconnect3_sdk
- id: wifi
  from: wiseconnect3_sdk
- id: wifi_resources
  from: wiseconnect3_sdk
- id: dmd_memlcd
- id: freertos
- id: glib
- id: sparkfun_6dof_imu_bmi270
  from: third_party_hw_drivers
