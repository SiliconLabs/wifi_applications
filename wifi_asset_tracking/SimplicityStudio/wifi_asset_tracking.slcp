  from: wiseconnect3_sdk
- id: network_manager
  from: wiseconnect3_sdk
- id: sl_calendar
  from: wiseconnect3_sdk
- id: sl_i2c
  from: wiseconnect3_sdk
- id: sl_si70xx
  from: wiseconnect3_sdk
